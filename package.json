    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
